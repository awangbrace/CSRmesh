<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".UserActivity">

    <include layout="@layout/actionbar_user"/>

    <FrameLayout
        android:layout_below="@+id/barUserContent"
        android:layout_above="@+id/atyUserDisposeBtn"
        android:id="@+id/atySetUserFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <Button
        android:id="@+id/atyUserDisposeBtn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/selector_base_btn"
        android:textColor="@color/white"
        android:textSize="20sp"/>

</RelativeLayout>
